<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=netstandard">
    <Color x:Key="tColorRaw">#003472</Color>
    <Color x:Key="fColorRaw">#96CE54</Color>
    <SolidColorBrush x:Key="tColor" Color="{DynamicResource tColorRaw}" />
    <SolidColorBrush x:Key="fColor" Color="{DynamicResource fColorRaw}" />
    <Color x:Key="fontTColorRaw">#8cc04a</Color>
    <Color x:Key="fontFColorRaw">#07336a</Color>
    <SolidColorBrush x:Key="fontTColor" Color="{DynamicResource fontTColorRaw}" />
    <SolidColorBrush x:Key="fontFColor" Color="{DynamicResource fontFColorRaw}" />
    <sys:Double x:Key="minOpacityValue">0.2</sys:Double>
    <sys:Double x:Key="goodOpacityValue">0.7</sys:Double>
    <SolidColorBrush
        x:Key="mainTimeFontColor"
        Opacity="1"
        Color="{DynamicResource fontTColorRaw}" />
    <SolidColorBrush
        x:Key="mainBackColor"
        Opacity="1"
        Color="{DynamicResource tColorRaw}" />
    <Style TargetType="Ellipse">
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect
                    BlurRadius="24"
                    ShadowDepth="-4"
                    Color="#000" />
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>